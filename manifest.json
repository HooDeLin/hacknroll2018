{
    "manifest_version": 2,
  
    "name": "NoEx",
    "description": "This extension blocks your ex-boyfriend/girlfriend photos",
    "version": "0.1",
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_icon": "noex.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "tabs",
      "http://*/",
      "https://*/",
      "activeTab",
      "storage"
    ]
}
